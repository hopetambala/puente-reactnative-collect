{
  "expo": {
    "name": "puente-reactnative-collect",
    "slug": "puente-reactnative-collect",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "0.3.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "0.3.0",
      "bundleIdentifier": "com.puente.collect",
      "infoPlist":{
        "NSCameraUsageDescription": "Would you like to allow Puente access to your camera roll?"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "android": {
      "versionCode": 370000300,
      "package": "com.puente.collect"
    }
  }
}
